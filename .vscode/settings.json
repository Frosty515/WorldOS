{
    "files.associations": {
        "system_error": "cpp",
        "thread": "cpp",
        "time.h": "c"
    },
    "makefile.dryrunSwitches": [
        "boot-iso"
    ],
    "makefile.makefilePath": "./Makefile",
    "makefile.makePath": "make",
    "makefile.phonyOnlyTargets": true,
    "C_Cpp.errorSquiggles": "enabled",
    "cSpell.words": [
        "framebuffer",
        "HHDM",
        "kfree",
        "kmalloc",
        "krealloc",
        "KVPM",
        "libgcc",
        "PPFA",
        "RSDP",
        "TMPFS",
        "Unallocate",
        "Unreserve",
        "WORLDOS",
        "XSDT"
    ],
    "git.ignoreLimitWarning": true,
    "C_Cpp.codeAnalysis.clangTidy.checks.disabled": [
        "clang-diagnostic-implicit-exception-spec-mismatch",
        "clang-diagnostic-incompatible-library-redeclaration",
        "clang-diagnostic-error"
    ],
    "C_Cpp.files.exclude": {
        "**/toolchain" : true,
        "**/.vscode": true,
        "**/.vs": true
    }
}